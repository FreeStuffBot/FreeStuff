{
  "pipeline": {
    "build": {
      "dependsOn": [ "^build" ],
      "outputs": [ "dist/**" ]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": [ "build" ]
    },
    "deploy": {
      "dependsOn": [ "build", "test", "lint" ]
    },
    "dev": {
      "cache": false
    }
  }
}